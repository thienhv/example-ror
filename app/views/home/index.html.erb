<%= form_for @search_developer_form, url: '/' do |f| %>
  <%= f.select :programming_language_id, ProgrammingLanguage.pluck(:name, :id), prompt: 'Select Developer Programming', name: 'Developer Programming' %>
  <%= f.select :language_id, Language.pluck(:code, :id), prompt: 'Select Language', name: 'Language' %>
  <%= f.submit 'Search' %>
<% end %>

<ul>
<% if @developers %>
  <% @developers.each do |developer| %>
    <li><%= developer.email %></li>
  <% end %>
<% end %>
</ul>
